import java.util.Scanner;

public class divisor_02 {

	public static void main(String[] args) {
		int cnt = 0, sum;
		for(int i=4; i <=1000; i++) {
			sum = 0;
			for (int j=1; j<i; i++) {
				if(i%j == 0){
					cnt++;
					sum += j;
				}
				
				if(sum == i) {
					System.out.println(i);
				}
				
			}
			
			
			
			
			
			
		}
		
		Scanner sc = new Scanner(System.in);
		
		
		
		sc.close();
	}

}
